{
  "name": "int_yuansfer_sfra",
  "version": "21.2.0",
  "description": "Yuansfer Payment Plugin",
  "main": "index.js",
  "scripts": {
    "compile:scss": "sgmf-scripts --compile css",
    "compile:js": "sgmf-scripts --compile js",
    "compile:fonts": "node bin/Makefile compileFonts",
    "lint": "sgmf-scripts --lint js",
    "lint:fix": "eslint --fix Cartridges/**",
    "upload": "sgmf-scripts --upload",
    "uploadCartridge": "sgmf-scripts --uploadCartridge int_yuansfer_sfra && sgmf-scripts --uploadCartridge bm_yuansfer",
    "watch": "sgmf-scripts --watch",
    "watch:static": "sgmf-scripts --watch static",
    "test:sfra": "mocha ./tests/sfra-integration-test",
    "test": "mocha ./tests/sfra-unit-test"
  },
  "author": "Shawn Zheng <shawn@yuansfer.com>",
  "license": "ISC",
  "devDependencies": {
    "lint": "^0.7.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^3.19.0",
    "eslint-auto-fix": "^1.0.3",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-sitegenesis": "~1.0.0",
    "mocha": "^8.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "sgmf-scripts": "^2.3.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.5.0",
    "superagent": "^5.3.1"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "packageName": "app_storefront_base",
  "babel": {
    "presets": [
      "env"
    ]
  }
}
